{
  "name": "mithril-redux-starter-webpack",
  "version": "0.1.10",
  "description": "Mithril Redux + Webpack",
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "watch": "NODE_ENV=development webpack-dev-server --content-base ./dist/ --inline --hot --host=0.0.0.0 --port=9000",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "prebuild": "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "test": "babel-node spec/support/test.js",
    "lint": "eslint ./src ./spec"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "baggage-loader": "^0.2.4",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-airbnb-base": "^4.0.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch": "^1.1.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "html-loader": "^0.4.3",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.6.5",
    "jasmine": "^2.4.1",
    "jasmine-expect": "^2.0.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "lost": "^7.0.3",
    "midux": "0.1.7",
    "mithril": "^0.2.5",
    "postcss": "^5.1.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-extend": "^1.0.4",
    "postcss-functions": "^2.1.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.3",
    "rucksack-css": "^0.8.6",
    "sanitize.css": "^4.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
